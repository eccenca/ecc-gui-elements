{
  "name": "ecc-gui-elements",
  "version": "1.16.4",
  "description": "Collection of low-level GUI elements like Buttons, Icons or Alerts. Also includes core styles for those elements.",
  "main": "es5/component.js",
  "es5": "es5/component.js",
  "es6": "index.js",
  "scripts": {
    "test": "gulp test && node-sass src/main.scss",
    "lint": "gulp lint",
    "cover": "gulp cover",
    "start": "gulp",
    "build": "gulp build",
    "copy-mdi": "mkdir -p dist/fonts/materialicons && cp -fru node_modules/material-design-icons/iconfont/* dist/fonts/materialicons",
    "copy-roboto": "mkdir -p dist/fonts/roboto && cp -fru node_modules/roboto/fonts/* dist/fonts/roboto",
    "init": "ecc-link-dotfiles",
    "prepublish": "git submodule update --init --recursive && npm run init && gulp vis && npm run copy-mdi && npm run copy-roboto && gulp && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.eccenca.com/elds-ui/ecc-gui-elements"
  },
  "author": "Michael Haschke <michael.haschke@eccenca.com>",
  "license": "ISC",
  "dependencies": {
    "classnames": "^2.2.5",
    "ecc-messagebus": "^3.3.2",
    "ecc-style-core": "^1.7.3",
    "emitter-component": "^1.1.1",
    "hammerjs": "^2.0.6",
    "keycharm": "^0.2.0",
    "lodash": "^4.16.4",
    "material-design-lite": "git://github.com/eccenca/material-design-lite.git#v1.1.3-fixed",
    "moment": "^2.12.0",
    "propagating-hammerjs": "^1.4.6",
    "react-mdl": "1.5.2",
    "react-select": "1.0.0-rc.2",
    "vis": "4.16.1"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0"
  },
  "devDependencies": {
    "ecc-dotfiles": "^1.6.0",
    "ecc-gulp-tasks": "^3.0.0-alpha",
    "ecc-resparqler": "^0.1.7",
    "ecc-test-helpers": "^15.0.0",
    "ecc-uitest-helpers": "^15.0.1",
    "gulp": "^3.9.0",
    "gulp-insert": "^0.5.0",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.4",
    "gulp-sass": "^2.1.1",
    "material-design-icons": "git://github.com/google/material-design-icons#2.2.3",
    "node-sass": "^3.4.0",
    "roboto": "git://github.com/FontFaceKit/roboto.git#v1.1.0",
    "react": "^15.0.0",
    "react-dom": "^15.0.0"
  }
}
